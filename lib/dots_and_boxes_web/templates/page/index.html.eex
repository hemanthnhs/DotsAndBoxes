<%= form_for @conn, "/game/", fn f -> %>
<div class="col-5">
  <label>
    Game name : <%= text_input f, :game_name, class: "form-control" %>
  </label>
  </div>
  <div class="col-5">
  <label>
    Player name : <%= text_input f, :player_name,  class: "form-control" %>
  </label>
  </div>
    <button class="offset-1 btn btn-primary">Begin Game</button>
<% end %>